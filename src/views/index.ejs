<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Pokémon List</title>
</head>
<body>
  <h1>Pokémon List</h1>
  <ul>
    <% pokemons.forEach(function(pokemon) { %>
      <li><a href="/stats/<%= pokemon.name %>"><%= pokemon.name %></a></li>
    <% }); %>
  </ul>
</body>
</html>
